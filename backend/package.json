    "@tsconfig/recommended": "^1.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.11",
    "@types/luxon": "^3.3.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^14.18.43",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "kysely-codegen": "^0.10.1",
